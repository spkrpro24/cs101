
public class Exercise7_4InterationFactorial {
	//public static void main(String[] args) {
	
	public static int factorial(int n) {
	    if (n == 0) {
	      return 1;
	    } else {
	      int recurse = factorial(n-1);
	      int result = n * recurse;
	      return result;
	
	    //public static void main(String[] args) {
	    	System.out.println("The Answer is " + recurse);
	    }
}
}

