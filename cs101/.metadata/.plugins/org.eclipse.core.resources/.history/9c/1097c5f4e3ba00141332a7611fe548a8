
public class Exercise12_5ArrayOfIntegers {	//inprogress
	//public static void main(String[] args) {
		

		   public static boolean areFactors(int n, int [] array)  {
			   for (int i = 0;i <array.length; i++)	{
				   if (array[i]%n != 0)	{
					   return false;
				   }
			   }
		        return true;
		   }
		}

