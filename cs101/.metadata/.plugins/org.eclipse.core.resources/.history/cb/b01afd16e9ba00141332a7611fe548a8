
public class Exercise12_5ArrayOfIntegers {	//inprogress
		

		   public static boolean areFactors(int n, int [] array)  {
			   for (int i = 0;i <array.length; i++)	{
				   if (array[i]%n != 0)	{
					   return false;
				   }
			   }
		        return true;
		   }
		   public static void main(String[] args) {
			   boolean display;
		        display = areFactors(4, 1980, 2000, 3000, 4000);
		        System.out.println("Is 1980 divisible by 4? The answer is " + display);
				
		}
}

